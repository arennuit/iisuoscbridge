      <Output id="{b4a1cf6f-054d-44c9-882b-d0da30034588}" name="leftHand_pos" />
      <Output id="{35acb3be-7fd5-4045-aaa9-665e5d7dc140}" name="rightHand_pos" />
      <Output id="{8e17cc70-8912-4385-b37a-21b6bfcbfaba}" name="planeOn_pos" />
      <Output id="{13209a6b-6f6a-4a04-b374-3878bef45096}" name="planeOff_pos" />
      <Output id="{637a7dcb-4c90-412f-a7fb-0d16988d0b4e}" name="leftHand_color" />
      <Output id="{75b98f90-241e-473e-a25f-763376519c47}" name="rightHand_color" />
      <Output id="{016f6d03-96cf-453b-b2f2-0de6cf3bb435}" name="leftHand_bang" />
      <Output id="{0ce79bf7-5d61-43c7-824f-8c1cafc34090}" name="rightHand_bang" />
      <CustomParameter id="{016f6d03-96cf-453b-b2f2-0de6cf3bb435}" />
      <CustomParameter id="{0ce79bf7-5d61-43c7-824f-8c1cafc34090}" />
      <Content><![CDATA[-----------------------------------------------------------------------------------------------------------------------------------
-- This script is provided with no guarantee, use it at your own risk.
-----------------------------------------------------------------------------------------------------------------------------------


function init()
	-- Important note:
	--   It's best to initialize ALL your variables here. Otherwise, you might get unexpected results.
	--   See the iisu Interaction Designer manual for more info.
	leftHand_front = false
	rightHand_front = false
	leftHand_frontKm1 = false
	rightHand_frontKm1 = false
	leftHand_bang = false
	rightHand_bang = false
end


function main()
  pelvis_pos = USER1_SKELETON_PARTS_Pelvis
	leftHand_pos = USER1_SKELETON_PARTS_LeftWrist
	rightHand_pos = USER1_SKELETON_PARTS_RightWrist

	-------------------------------------------------------------------
	-- Toggle the status of each hand based on the position, the threshold and hysteresis 
	-------------------------------------------------------------------
	if (leftHand_front)	then
	  if (leftHand_pos:front() - pelvis_pos:front() < (hand_front_threshold - hand_front_hysteresis) ) then
			leftHand_front = false
		end
	else
	  if ((leftHand_pos:front() - pelvis_pos:front()) > hand_front_threshold) then
			leftHand_front = true
		end
	end
	
	if (leftHand_front and not leftHand_frontKm1) then
		leftHand_bang = true
	else
		leftHand_bang = false
	end
	leftHand_frontKm1 = leftHand_front

	-- same for the right hand
	if(rightHand_front)	then
	  if(rightHand_pos:front() - pelvis_pos:front() < (hand_front_threshold - hand_front_hysteresis) ) then
			rightHand_front = false
		end
	else
	  if(rightHand_pos:front() - pelvis_pos:front() > hand_front_threshold) then
			rightHand_front = true
		end
	end
	
	if (rightHand_front and not rightHand_frontKm1) then
		rightHand_bang = true
	else
		rightHand_bang = false
	end
	rightHand_frontKm1 = rightHand_front
	
	-------------------------------------------------------------------
	-- visualization codes (disable the next lines to gain a bit on performance when using this script without the interaction designer tool)
	-------------------------------------------------------------------
	planeOn_pos = pelvis_pos +  SK.Axis_FRONT * hand_front_threshold
	planeOff_pos = pelvis_pos +  SK.Axis_FRONT * (hand_front_threshold - hand_front_hysteresis)
	
	local red = SK.Color4f(1.0, 0.0, 0.0, 1.0)
	local green = SK.Color4f(0.0, 1.0, 0.2, 1.0)
	if (leftHand_front) then
		leftHand_color = green
	else
		leftHand_color = red
	end	

	if(rightHand_front) then
		rightHand_color = green
	else
		rightHand_color = red
	end	
end
]]></Content>
